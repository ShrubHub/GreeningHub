  return(\
        image.reduceRegions(\{              \
        collection: regions,  \
        reducer: ee.Reducer.mean(),     \
        scale: 5000,\})\
        .select(['label', 'mean'], ['region', 'NDVI'], false)\
        .map(function(feature) \{\
          return(\
            feature.set(\
              'year', \
              image.get('year')));\
        \}) // add year property to feature.\
  );\
\};\
