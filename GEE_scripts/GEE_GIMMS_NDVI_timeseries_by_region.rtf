// Define function that returns an ImageCollection from all bands in an Image\
var imageToImageCollection = function(inputImage)\{\
  // extract band names\
   var bandNames = inputImage.bandNames();\
   // map over band names extract band, assign name for band in image (NDVI)\
   // and add date from bandname string\
   var returnImageCollection = bandNames.map(function(band_to_extract)\{\
     return ee.Image(inputImage.select([band_to_extract], ["NDVI"]))\
     .set("system:time_start", ee.Date.parse("dd/MM/yyyy", band_to_extract).format("yyyy-MM-dd"));\
   \});\
   return returnImageCollection;\
\};\
\
// Turn GIMMS image into a collection\
